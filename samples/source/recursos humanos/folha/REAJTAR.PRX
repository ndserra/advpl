aFields := {}
AADD(aFields,{"FA_FILIAL" ,"C",FWGETTAMFILIAL,0})
AADD(aFields,{"FA_MAT"    ,"C",06,0})
AADD(aFields,{"FA_CC"     ,"C",09,0})
