	aAdd(aBanco,RTRIM(SA6->A6_MUN)+" - "+A6_EST)
Else
	aAdd(aBanco,STR0014)		//"** Nao Cadastrado **"
	aAdd(aBanco," ")
	aAdd(aBanco," ")
Endif
dbSelectArea ( "SRA" )

Return .T.