						cNameFile		,; //Nome do arquivo da tabela temporaria 
						cAliasPer		,;  //Alias do Periodo de Apontamento 
					    aPeriodos		; //Periodos de Apontamento
					  ) 
	
				  			
RestArea(aArea)

Return(.T.) 



