			INNER JOIN %exp:cNameFilePer% PERAPON
			ON 	( SPC.PC_DATA BETWEEN  PERAPON.PO_DATAINI AND PERAPON.PO_DATAFIM) AND ( PERAPON.PO_FILIAL = SPC.PC_FILIAL )	
