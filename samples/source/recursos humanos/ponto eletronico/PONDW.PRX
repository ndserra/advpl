		0 AS HRSEXTRABO,
		0 AS FALTATRABO,
		0 AS HRSAFAST,
		0 AS HRSFERIAS,
		PERAPON.PO_FILIAL AS PO_FILIAL, PERAPON.SEQUENCIA  AS SEQUENCIA, PERAPON.DESCPER  AS DESCPER, PERAPON.PO_DATAINI AS PO_DATAINI, PERAPON.PO_DATAFIM AS PO_DATAFIM  
			FROM 	 %Table:SPC% SPC 
