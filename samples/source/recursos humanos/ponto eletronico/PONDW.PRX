			                    aPeriodos		; //Periodos de Apontamento
							  )  					  
					    
				  
Local aArea	:= GetArea()

//-- Posiciona no Primeiro Registro do Periodo de Apontamento
(cAliasPer)->(DbGotop())

        
		PonDwHorasApo(	cAlias					,; //Alias da tabela de Origem de Dados
